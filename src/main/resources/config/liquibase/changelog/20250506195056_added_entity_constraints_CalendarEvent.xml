<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity CalendarEvent.
    -->
    <changeSet id="20250506195056-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="event_id"
                                 baseTableName="calendar_event"
                                 constraintName="fk_calendar_event__event_id"
                                 referencedColumnNames="id"
                                 referencedTableName="event"
                                 />

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="calendar_event"
                                 constraintName="fk_calendar_event__created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"
                                 />
    </changeSet>
</databaseChangeLog>
