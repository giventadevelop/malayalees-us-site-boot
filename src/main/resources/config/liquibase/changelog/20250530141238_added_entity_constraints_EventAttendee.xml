<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity EventAttendee.
    -->
    <changeSet id="20250530141238-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="event_id"
                                 baseTableName="event_attendee"
                                 constraintName="fk_event_attendee__event_id"
                                 referencedColumnNames="id"
                                 referencedTableName="event_details"
                                 />

        <addForeignKeyConstraint baseColumnNames="attendee_id"
                                 baseTableName="event_attendee"
                                 constraintName="fk_event_attendee__attendee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_profile"
                                 />
    </changeSet>
</databaseChangeLog>
